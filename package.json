{
  "name": "lint-my-app",
  "version": "0.0.0-development",
  "description": "So you can write your app instead of lint it!",
  "keywords": [
    "check",
    "code",
    "eslint",
    "fix",
    "fixjson",
    "format",
    "imagemin",
    "lint",
    "lint-staged",
    "optimize",
    "quality",
    "sort",
    "staged",
    "stylelint",
    "validate"
  ],
  "homepage": "https://saiichihashimoto.github.io/lint-my-app/",
  "bugs": {
    "url": "https://github.com/saiichihashimoto/lint-my-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saiichihashimoto/lint-my-app.git"
  },
  "license": "MIT",
  "main": "lint-staged.config.js",
  "bin": "bin/lint-my-app.js",
  "scripts": {
    "fix": "npm run lint-my-app -- fix",
    "lint": "npm run lint-my-app -- lint",
    "lint-my-app": "./bin/lint-my-app.js",
    "staged": "npm run lint-my-app -- staged",
    "test": "jest --env node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "saiichihashimoto"
  },
  "dependencies": {
    "commander": "2.19.0",
    "cosmiconfig": "^5.0.7",
    "eslint": "5.13.0",
    "execa": "1.0.0",
    "fixjson": "1.1.0",
    "imagemin-lint-staged": "^0.4.0",
    "jsonlint": "1.6.3",
    "lint-staged": "8.1.3",
    "listr": "0.14.3",
    "pkg-ok": "2.3.1",
    "sort-package-json": "1.19.0",
    "stylelint": "9.10.1"
  },
  "devDependencies": {
    "cz-conventional-changelog": "2.1.0",
    "eslint-config-saiichihashimoto": "1.0.6",
    "husky": "1.3.1",
    "jest": "^24.1.0"
  }
}
