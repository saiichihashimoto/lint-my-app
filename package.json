{
  "name": "lint-my-app",
  "version": "0.0.0-development",
  "homepage": "https://github.com/saiichihashimoto/lint-my-app#readme",
  "bugs": {
    "url": "https://github.com/saiichihashimoto/lint-my-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saiichihashimoto/lint-my-app.git"
  },
  "main": "lint-staged.config.js",
  "directories": {
    "bin": "./bin"
  },
  "scripts": {
    "test": "run-s test:*",
    "test:fix": "sh ./bin/fix-my-app",
    "test:lint": "sh ./bin/lint-my-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "saiichihashimoto"
  },
  "dependencies": {
    "eslint": "5.13.0",
    "fixjson": "1.1.0",
    "imagemin-lint-staged": "^0.4.0",
    "jsonlint": "1.6.3",
    "lint-staged": "8.1.3",
    "pkg-ok": "2.3.1",
    "sort-package-json": "1.18.0",
    "stylelint": "9.10.1"
  },
  "devDependencies": {
    "cz-conventional-changelog": "2.1.0",
    "eslint-config-saiichihashimoto": "1.0.1",
    "husky": "1.3.1",
    "npm-run-all": "4.1.5"
  }
}
